      xmlns:y11="http://schemas.microsoft.com/SMI/2011/WindowsSettings"
      xmlns:y16="http://schemas.microsoft.com/SMI/2016/WindowsSettings"
      xmlns:y19="http://schemas.microsoft.com/SMI/2019/WindowsSettings">
      <y11:disableWindowFiltering>true</y11:disableWindowFiltering>
      <y16:dpiAwareness>PerMonitorV2</y16:dpiAwareness>
      <y16:longPathAware>true</y16:longPathAware>
      <y19:activeCodePage>UTF-8</y19:activeCodePage>
    </v3:windowsSettings>
  </v3:application>

</assembly>
