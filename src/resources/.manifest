  xmlns="urn:schemas-microsoft-com:asm.v1"
  xmlns:v2="urn:schemas-microsoft-com:asm.v2"
  xmlns:v3="urn:schemas-microsoft-com:asm.v3"
  manifestVersion="1.0">

  <assemblyIdentity type="win32" version="1.0.0.0" name="Flipeador.EnvVars"/>

  <description>Environment Variables Editor</description>

  <dependency>
    <dependentAssembly>
      <assemblyIdentity
        type="win32"
        name="Microsoft.Windows.Common-Controls"
        version="6.0.0.0"
        processorArchitecture="*"
        publicKeyToken="6595b64144ccf1df"
        language="*"/>
    </dependentAssembly>
  </dependency>

  <compatibility xmlns="urn:schemas-microsoft-com:compatibility.v1">
    <application>
      <!-- W10 | W11 | WS2016 | WS2019 | WS2022 -->
      <supportedOS Id="{8e0f7a12-bfb3-4fe8-b9a5-48fd50a15a9a}"/>
    </application>
  </compatibility>

  <v2:trustInfo>
